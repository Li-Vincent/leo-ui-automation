<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Suite" thread-count="1" parallel="instances"
       verbose="3">
    <listeners>
        <listener class-name="org.uncommons.reportng.HTMLReporter"/>
        <listener class-name="automation.listener.RetryListener"/>
        <listener class-name="automation.listener.TestResultListener"/>
    </listeners>
    <test name="Test" preserve-order="true">
        <packages>
            <package name="automation.ui.datafactory"></package>
        </packages>
    </test>
</suite>
